<h3>ルーム新規作成</h3>
 
    <%= form_with model: @room do |f| %>
      <table>
        <tr>
          <th><%= f.label :room_name ,"ルーム名" %></th>
          <td><%= f.text_field :room_name %></td>
        </tr>
 
        <tr>
          <th><%= f.label :room_info ,"ルーム紹介" %></th>
          <td><%= f.text_area :room_info %></td>
        </tr>
 
        <tr>
          <th><%= f.label :room_price ,"料金" %></th>
          <td><%= f.number_field :room_price , min: 1 %></td>
        </tr>
 
        <tr>
          <th><%= f.label :room_place ,"住所" %></th>
          <td><%= f.text_field :room_place %></td>
        </tr>
        
        <tr>
          <th><%= f.label :room_image ,"ルーム画像" %></th>
          <td><%= f.file_field :room_image %></td>
        </tr>
        <%= f.hidden_field :user_id, value: current_user.id %>
      </table>
      <div>
        <ul>
          <li><%= f.submit "登録完了する"%></li>
          <li><%= link_to "ルーム一覧に戻る", :rooms %></li>
        </ul>
      </div>
    <% end %>